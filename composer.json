{
    "name": "nojimage/cakephp-admin-lte",
    "description": "AdminLTE theme template for CakePHP 3.0",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp", "bootstrap", "template", "theme"
    ],
    "homepage": "http://elasticconsultants.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Takashi Nojima",
            "homepage": "http://php-tips.com"
        }
    ],
    "extra": {
        "installer-name": "AdminLTE"
    },
    "require": {
        "php": ">=5.6",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "^3.4",
        "friendsofcake/bootstrap-ui": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "AdminLTE\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminLTE\\Test\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": "AdminLTE\\Console\\Installer::postInstall"
    }
}
